<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyLoginEditText">
        <attr name="maxLength" format="integer" />
        <attr name="hint" format="string" />
        <attr name="showClearIcon" format="boolean" />
        <attr name="inputType" format="enum" >
            <enum name="password" value="2"/>
            <enum name="number" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ItemViewWithDrawable">
        <attr name="icon" format="reference" />
        <attr name="text" format="string|reference" />
        <attr name="show_arrow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AppendableTextView">
        <attr name="key_text" format="string|reference" />
    </declare-styleable>
    <declare-styleable name="TitleTextWithCube">
        <attr name="title_text" format="string|reference" />
        <attr name="textSize" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="SearchBarView">
        <attr name="hint_text" format="string|reference" />
    </declare-styleable>
    <declare-styleable name="StartEndTextView">
        <attr name="start_text" format="string|reference" />
        <attr name="value_text" format="string|reference" />
    </declare-styleable>
</resources>